<!DOCTYPE html>
<html lang="en">
  <head>
    <% include partials/_htmlhead %>
  </head>
  <body>
      <% include partials/_header %>
      <ul>
        <% for (url in urls) { %>
          <li><%= urls[url] %> - <%= url %></li>
          <form method="POST" action="/urls/<%=url%>/delete">
            <button type="submit" value="Delete">Delete &#x1F5D1</button>
            <a href=/urls/<%=url%>/edit>Edit</a>
          </form>
        <% }; %>
      </ul>
  </body>
</html>