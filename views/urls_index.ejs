  <!DOCTYPE html>
<html lang="en">


<head>
  <title>TinyApp</title>
    <% include partials/_header %>

</head>


<body>

    <!-- Add your EJS code here -->

    <ul>
      <% for (url in urls) { %>
        <li><%= urls[url] %> - <%= url %></li>
        <form method="POST" action="/urls/<%=url%>/delete">
          <button type="submit" value="Delete">Delete &#x1F5D1</button>
          <a href=/urls/<%=url%>/edit>Edit</a>
        </form>
      <% }; %>
    </ul>

</body>

</html>